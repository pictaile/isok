<html>
<head>
    <title>isok</title>
    <link rel="shortcut icon" href="./favicon.ico" type="image/x-icon"/>
    <meta charset="utf-8">

    {% javascripts '@StaticBundle/Resources/public/js/*'  output='js/compiled/main.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% stylesheets '@StaticBundle/Resources/public/css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {#<link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i&subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext" rel="stylesheet">#}
    {#<link rel="stylesheet" href="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css" />#}
    {#<script src="/libs/jquery.min.js"></script>#}

</head>

<body>

    <div class="container-wrapper">

        <div class="main-wrapper">
            <div class="header-wrapper">
                <div class="logo-wrapper">
                    {% image '@StaticBundle/Resources/public/img/logo.png' %}
                        <a href="/{% if app.environment == "dev" %}app_dev.php{% endif %}"><img src="{{ asset_url }}" alt="Example" /></a>
                    {% endimage %}
                </div>

                <div class="search-wrapper form-group">
                    {% block search %}{% endblock %}
                </div>

                <div class="switchers-wrapper">
                    {% block switchers %}
                        <a href="/{% if app.environment == "dev" %}app_dev.php{% endif %}/business/" class="btn btn-primary" >Бизнесу</a>
                        <a href="/{% if app.environment == "dev" %}app_dev.php{% endif %}/client/"  class="btn btn-success" > Частным лицам</a>
                    {% endblock %}
                </div>

            </div>


            <div class="main-content">
                {% block content %}

                    <div class="description-wrapper">
                        Описание
                    </div>

                    <div class="video-wrapper">
                        {% image '@StaticBundle/Resources/public/img/not-found.png' %}
                        <img src="{{ asset_url }}" alt="Example" />
                        {% endimage %}
                    </div>

                {% endblock %}
            </div>



        </div>


        <div class="footer-wrapper">
            <div class="our-contacts">Наши контакты:</div>
            <p>Email: test@te.te </p>
            <p>Телефон: +380971112233</p>
        </div>



    </div>

</body>
